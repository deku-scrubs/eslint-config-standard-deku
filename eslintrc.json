{
  "ecmaFeatures": {
    "jsx": true
  },

  "plugins": [
    "deku"
  ],

  "rules": {
    "deku/display-name": 0,
    "deku/jsx-boolean-value": 2,
    "deku/jsx-no-undef": 2,
    "deku/jsx-quotes": [2, "single", "avoid-escape"],
    "deku/jsx-sort-prop-types": 0,
    "deku/jsx-sort-props": 0,
    "deku/jsx-uses-deku": [2, {"pragma": "element"}],
    "deku/jsx-uses-vars": 2,
    "deku/no-did-mount-set-state": 0,
    "deku/no-did-update-set-state": 2,
    "deku/no-multi-comp": 0,
    "deku/no-unknown-property": 2,
    "deku/prop-types": 2,
    "deku/self-closing-comp": 2,
    "deku/sort-comp": 0,
    "deku/wrap-multilines": 2
  }
}
